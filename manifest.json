{
  "manifest_version": 2,

  "name": "FT, ad free",
  "description": "Disables adblock countermeasures on ft.com.",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon128.png",
    "default_title": "FT, ad free"
  },
  "content_scripts": [
    {
      "matches":["https://www.ft.com/*"],
      "css": ["ft.css"]
    }
  ],
  "permissions": [
    "https://www.ft.com/*/"
  ]
}
